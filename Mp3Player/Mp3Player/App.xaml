<Application x:Class="Mp3Player.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Mp3Player"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="editImage" UriSource="images\edit.png"></BitmapImage>
            <BitmapImage x:Key="openFileImage" UriSource="images\open_file.png"></BitmapImage>
            <BitmapImage x:Key="musicNoteImage" UriSource="images\musicnote.png"></BitmapImage>
            <BitmapImage x:Key="stopImage" UriSource="images\stop-icon.png"></BitmapImage>
            <BitmapImage x:Key="playImage" UriSource="images\media-play.png"></BitmapImage>
            <BitmapImage x:Key="pauseImage" UriSource="images\pause-icon.png"></BitmapImage>
        </ResourceDictionary>
    </Application.Resources>
    
    
</Application>
